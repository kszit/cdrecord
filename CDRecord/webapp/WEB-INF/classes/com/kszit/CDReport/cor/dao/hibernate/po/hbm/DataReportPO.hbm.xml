<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.kszit.CDReport.cor.dao.hibernate.po">
	<class name="DataReportPO" table="dataReport">
        <id name="id" column="ID">
            <generator class="native"/>
        </id>
        <property name="bindId"/>
        <property name="repotBindid"/>
        <property name="year"/>
        <property name="periods"/>
        <property name="madeManName"/>
        <property name="verifyManName"/>
        <property name="madeManDate"/>
        <property name="verifyManDate"/>
        <property name="orderIndex"/>
        <property name="createDepBindid"/>
        <property name="verify"/>
        <property name="appearDate"/>
        <property name="updateManName"/>
        <property name="updateDate"/>        
    </class>
</hibernate-mapping>